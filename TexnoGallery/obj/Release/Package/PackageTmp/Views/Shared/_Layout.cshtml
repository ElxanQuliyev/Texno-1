<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Texno Gallery texnologiya dünyası komputerlər, noutbuklar, prosessorlar">
    <meta name="keywords" content="Pc topla, Komputerlər, noutbuklar, Qulaqlıq, Texnologiya,Sistem bloku, Prosessor, Amd, İntel,Hp">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Texno Gallery - TexnoGallery.az</title>
    <link rel="icon" href="~/Uploads/albabaİcon.png" type="image/x-icon">
    <link rel="stylesheet" href="~/css/bootstrap.css">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/owl.carousel.css">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">
    <script src="https://kit.fontawesome.com/a5a64ef4c6.js"></script>
    @RenderSection("mainCss", required: false)

    <link rel="stylesheet" href="~/css/style.css">
</head>

<body>

    <!--# Top Header Start-->
    <div class="top-header">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="top-right">
                        <ul>
                            <li>
                                <a href="https://www.facebook.com/texnogallery28/" target="_blank">
                                    <img style="width: 25px" src="~/images/facenew.png" alt="">
                                </a>
                            </li>
                            <li>
                                <a href="https://www.instagram.com/texnogallery/" target="_blank">
                                    <img style="width: 28px" src="~/images/580b57fcd9996e24bc43c521.png" alt="">
                                </a>
                            </li>
                            <li>
                                <a href="https://www.youtube.com/channel/UCI3F06OFocdcvv9M1lPA6WQ" target="_blank">
                                    <img style="width: 30px" src="~/images/580b57fcd9996e24bc43c545.png" alt="">
                                </a>
                            </li>
                            <li class="pr-0"><a href="#">En |</a></li>
                            <li class="pr-0"><a href="#">Az |</a></li>
                            <li><a href="#">Ru</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12 ">
                    <div class="top-left text-right ">
                        @if (Session["user"] == null)
                        {
                            <ul>
                                <li><a href="@Url.Action("Login", "MainAccount")"><i class="fas fa-lock"></i>Login</a></li>
                                <li><a href="@Url.Action("Register", "MainAccount")"><i class="fas fa-key"></i> Registred</a></li>
                            </ul>
                        }
                        else
                        {
                            var appUser = Session["user"] as TexnoGallery.Models.ApplicationUser;
                            <div class="dropdown">
                                <button class="btn dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    @appUser.FirstName
                                </button>
                                <div class="dropdown-menu mainDropdown" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="@Url.Action("Profiles","MainAccount")">Profile</a>
                                    <a class="dropdown-item" href="@Url.Action("Logout","MainAccount")">Logout</a>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--# Top Header End-->
    <!--#Header Start-->
    <div class="site-header-main">
        <nav class="main-nav">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-md-6 col-6 col-lg-4 col-xl-4">
                        <a href="@Url.Action("Index","Home")"><img src="~/images/albaba.png" style=" width:170px " alt="logo"></a>
                    </div><!-- .site-branding -->
                    <div class="col-lg-6 menu col-xl-6">
                        <nav class="navbar navbar-expand-lg pt-2">

                            <button class="navbar-toggler" type="button" data-toggle="collapse"
                                    data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup"
                                    aria-expanded="false" aria-label="Toggle navigation">
                                <span class="navbar-toggler-icon"></span>
                            </button>
                            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                                <div class="navbar-nav  main-menu">
                                    <a class="nav-item nav-link text-white mr-4 menu-a" href="@Url.Action("Index","Home")">
                                        Ana Səhifə  <div class="active"></div>
                                        <span class="sr-only">(current)</span>
                                    </a>
                                    @*<a class="nav-item nav-link text-white mr-4 menu-a" href="@Url.Action("Product", "Product")">
                                            Məhsullar <span class="sr-only">(current)</span>
                                        </a>*@
                                    <a class="nav-item nav-link text-white mr-4 menu-a" href="@Url.Action("About","Home")">
                                        Haqqımızda <span class="sr-only">(current)</span>
                                    </a>
                                    <a class="nav-item nav-link text-white menu-a" href="@Url.Action("Contact","Home")">
                                        Əlaqə <span class="sr-only">(current)</span>
                                    </a>
                                </div>
                            </div>
                        </nav>

                    </div>
                    <!-- .site-header-menu -->


                    <div class="col-4 col-sm-4 col-md-4 col-lg-2 col-xl-2 hidden-sm">
                        <div class="header-cart" id="cart">

                            <a href="#"><i class="fas fa-shopping-cart icofont-cart"></i></a>
                            <div class="items"> <span></span> </div>
                        </div>
                    </div>
                    <!-- HeaderIconn -->

                    <div class="col-2 col-sm-2 col-md-2">
                        <div id="mySidenav" class="sidenav mr-2">
                            <a href="javascript:void(0)" class="closebtn mr-3" onclick="closeNav()">&times;</a>
                            <div class="lang">
                                <ul>
                                    <li>
                                        <a href="#">Az</a>
                                    </li>
                                    <li>
                                        <a href="#">En</a>
                                    </li>
                                    <li>
                                        <a href="#">Ru</a>
                                    </li>
                                </ul>
                            </div>
                            <div class="log">
                                <ul>
                                    <li><a href="@Url.Action("Login","MainAccount")"><i class="fas fa-lock"></i> Login</a></li>
                                    <li><a href="@Url.Action("Register","MainAccount")"><i class="fas fa-key"></i> Registred</a></li>
                                </ul>
                            </div>

                            <a href="@Url.Action("Index","Home")">Ana Səhifə</a>
                            <a href="@Url.Action("About","Home")">Haqqımızda</a>
                            <a href="@Url.Action("Contact","Home")">Əlaqə</a>
                        </div>
                        <span style="font-size:30px;cursor:pointer" class="spanIcon" onclick="openNav()">&#9776;</span>
                    </div>
                    <!-- HeaderIconn End-->
                </div><!-- .row -->
            </div>
        </nav>
    </div>
    @Html.Partial("ProductCategory")
    <!--#Header End-->
    <div class="bg-modal">
        <div class="modal-content">
            <div class="modal-name">
                <p>SƏBƏT</p>
            </div>
            <div class="close-modal">+</div>

            <div class="modal-item">
                <div class="empty-basket text-center mt-5">
                    <i style="color:#ffc107" class="fas fa-shopping-basket fa-3x"></i>
                    <h1 class=" ">Səbət boşdur</h1>
                    <a class="btn btn-warning" href="@Url.Action("index","Home")">Ana Sehife</a>

                </div>
            </div>


        </div>
    </div>
    @RenderBody()
    <!--Footer-->
