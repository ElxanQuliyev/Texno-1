    <script src="~/Areas/Admin/assets/js/prism/prism.min.js"></script>
    @*<script src="~/Areas/Admin/assets/js/clipboard/clipboard.min.js"></script>*@
    <script src="~/Areas/Admin/assets/js/counter/jquery.waypoints.min.js"></script>
    <script src="~/Areas/Admin/assets/js/counter/jquery.counterup.min.js"></script>
    <script src="~/Areas/Admin/assets/js/counter/counter-custom.js"></script>
    @*<script src="~/Areas/Admin/assets/js/custom-card/custom-card.js"></script>*@
    <script src="~/Areas/Admin/assets/js/notify/bootstrap-notify.min.js"></script>
    @*<script src="~/Areas/Admin/assets/js/dashboard/default.js"></script>*@
    <script src="~/Areas/Admin/assets/js/notify/index.js"></script>
    <script src="~/Areas/Admin/assets/js/typeahead/handlebars.js"></script>
    @*<script src="~/Areas/Admin/assets/js/typeahead/typeahead.bundle.js"></script>*@
    @*<script src="~/Areas/Admin/assets/js/typeahead/typeahead.custom.js"></script>*@
    @*<script src="~/Areas/Admin/assets/js/chat-menu.js"></script>*@
    <script src="~/Areas/Admin/assets/js/height-equal.js"></script>
    @*<script src="~/Areas/Admin/assets/js/tooltip-init.js"></script>*@
    @*<script src="~/Areas/Admin/assets/js/typeahead-search/handlebars.js"></script>*@
    @*<script src="~/Areas/Admin/assets/js/typeahead-search/typeahead-custom.js"></script>*@
    <!-- Plugins JS Ends-->
    <!-- Theme js-->

    <script src="~/Areas/Admin/assets/js/script.js"></script>





</body>

</html>
