<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="endless admin is super flexible, powerful, clean &amp; modern responsive bootstrap 4 admin template with unlimited possibilities.">
    <meta name="keywords" content="admin template, endless admin template, dashboard template, flat admin template, responsive admin template, web app">
    <meta name="author" content="pixelstrap">
    <link rel="icon" href="~/Uploads/albabaİcon.png" type="image/x-icon">
    <link rel="shortcut icon" href="~/Uploads/albabaİcon.png" type="image/x-icon">
    <title>Texno Gallery - Admin Panel</title>
    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Work+Sans:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Font Awesome-->
    <link href="~/Scripts/dropzone/dropzone.min.css" rel="stylesheet" />
    <!-- ico-font-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/icofont.css">
    <!-- Themify icon-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/themify.css">
    <!-- Flag icon-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/flag-icon.css">
    <!-- Feather icon-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/feather-icon.css">
    <!-- Plugins css start-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/prism.css">
    <!-- Plugins css Ends-->
    <!-- Bootstrap css-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/bootstrap.css">

    <!-- App css-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/style.css">
    @RenderSection("adminCss", required: false)

    <link id="color" rel="stylesheet" href="~/Areas/Admin/assets/css/light-1.css" media="screen">
    <link rel="stylesheet" href="~/Areas/Admin/assets/theme.css">
    <!-- Responsive css-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Admin/assets/css/responsive.css">
    <script src="https://kit.fontawesome.com/a5a64ef4c6.js"></script>
    <script src="~/Scripts/tinymce/tinymce.js"></script>
    <script>
        tinymce.init({
            selector: 'textarea',  // change this value according to your HTML
            auto_focus: 'element1'
        });
    </script>
</head>
<body>

    <!-- Loader ends-->
    <!-- page-wrapper Start-->
    <div class="page-wrapper">
        <!-- Page Header Start-->
        <div class="page-main-header">
            <div class="main-header-right row">
                <div class="main-header-left d-lg-none">
                </div>
                <div class="mobile-sidebar d-block">
                    <div class="media-body text-right switch-sm">
                        <label class="switch"><a href="#"><i id="sidebar-toggle" data-feather="align-left"></i></a></label>
                    </div>
                </div>
                <div class="nav-right col p-0">
                    <ul class="nav-menus">
                        <li>

                        </li>


                        <li class="onhover-dropdown">
                            <div class="media align-items-center">
                                <i class="fas fa-user fa-2x"></i>
                            </div>
                            <ul class="profile-dropdown onhover-show-div p-20">
                                <li><a href="@Url.Action("ChangePassword","AdminAccount")"><i data-feather="settings"></i>Settings</a></li>
                                <li><a href="@Url.Action("Login","AdminAccount")"><i data-feather="log-out"></i>Logout</a></li>
                            </ul>
                        </li>
                    </ul>
                    <div class="d-lg-none mobile-toggle pull-right"><i data-feather="more-horizontal"></i></div>
                </div>
                <script id="result-template" type="text/x-handlebars-template">
                    <div class="ProfileCard u-cf">
                        <div class="ProfileCard-avatar"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-airplay m-0"><path d="M5 17H4a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h16a2 2 0 0 1 2 2v10a2 2 0 0 1-2 2h-1"></path><polygon points="12 15 17 21 7 21 12 15"></polygon></svg></div>
                        <div class="ProfileCard-details">
                            <div class="ProfileCard-realName">{{name}}</div>
                        </div>
                    </div>
                </script>
                <script id="empty-template" type="text/x-handlebars-template">
                    <div class="EmptyMessage">Your search turned up 0 results. This most likely means the backend is down, yikes!</div>

                </script>
            </div>
        </div>
        <!-- Page Header Ends-->
        <!-- Page Body Start-->
        <div class="page-body-wrapper">
            <!-- Page Sidebar Start-->
            <div class="page-sidebar">
                <div class="main-header-left d-none d-lg-block mt-5">
                    <div class="logo-wrapper"><a href="@Url.Action("Index","Home")"><img style="width:200px" src="~/Uploads/albaba.png" alt=""></a></div>
                </div>
                <div class="sidebar custom-scrollbar">

                    <ul class="sidebar-menu mt-5">
                        <li><a class="sidebar-header" href="@Url.Action("Index","Home")"><i data-feather="home"></i><span>Dashboard</span></a></li>
                        <li>
                            <a class="sidebar-header" href="#"><i data-feather="airplay"></i><span>Ana Səhifə</span><i class="fas fa-chevron-right pull-right"></i></a>
                            <ul class="sidebar-submenu">
                                <li><a href="@Url.Action("Index","CategoryText")"><i class="fa fa-circle"></i><span>Kateqoriya</span></a></li>
                                <li><a href="@Url.Action("Index","SubCategories")"><i class="fa fa-circle"></i><span>Sub Category</span></a></li>
                                <li><a href="@Url.Action("Index","SlideImage")"><i class="fa fa-circle"></i><span>Slayd</span></a></li>
                                <li><a href="@Url.Action("Index","CategoryImage")"><i class="fa fa-circle"></i><span>Şəkil Kateqoriya</span></a></li>
                                <li><a href="@Url.Action("Index","Brends")"><i class="fa fa-circle"></i><span>Brendlər</span></a></li>

                            </ul>
                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","AddProducts")"><i data-feather="shopping-bag"></i><span>Məhsullar</span></a>

                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","AboutSite")"><i data-feather="disc"></i><span>Haqqımızda</span></a>

                        </li>
                        <li>
                            <a class="sidebar-header" href="@Url.Action("Index","ContactSite")"><i data-feather="sidebar"></i><span>Kontakt</span></a>

                        </li>
                        <li>
                        <li><a href="@Url.Action("Index","Orders")"><i class="fa fa-circle"></i><span>Sifarişlər</span></a></li>
                    </ul>
                </div>
            </div>
            <!-- Page Sidebar Ends-->

            <div class="page-body">

                <!-- Container-fluid starts-->
                <div class="container-fluid ">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card mt-5">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Container-fluid Ends-->
            </div>
            <!-- footer start-->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6 footer-copyright">
                            <p class="mb-0">Copyright 2019 ©</p>
                        </div>

                    </div>
                </div>
            </footer>
        </div>
    </div>
    <!-- latest jquery-->
    <script src="~/Areas/Admin/assets/js/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap js-->
    <script src="~/Areas/Admin/assets/js/bootstrap/bootstrap.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/dropzone/dropzone.min.js"></script>
    <script src="~/Areas/Admin/assets/js/bootstrap/popper.min.js"></script>
    @RenderSection("adminJs", false)

    <!-- feather icon js-->
    <script src="~/Areas/Admin/assets/js/icons/feather-icon/feather.min.js"></script>
    <script src="~/Areas/Admin/assets/js/icons/feather-icon/feather-icon.js"></script>
    <!-- Sidebar jquery-->
    <script src="~/Areas/Admin/assets/js/sidebar-menu.js"></script>
    <script src="~/Areas/Admin/assets/js/config.js"></script>
